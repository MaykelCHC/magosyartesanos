{% extends 'base.html.twig' %}

{% block title %}Producto{% endblock %}

{% block body %}
    <h1>Producto</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ producto.id }}</td>
            </tr>
            <tr>
                <th>Nombre</th>
                <td>{{ producto.nombre }}</td>
            </tr>
            <tr>
                <th>Imagenproducto</th>
                <td>{{ producto.imagenproducto }}</td>
            </tr>
            <tr>
                <th>Beneficioxventa</th>
                <td>{{ producto.beneficioxventa }}</td>
            </tr>
            <tr>
                <th>Tipomoneda</th>
                <td>{{ producto.tipomoneda }}</td>
            </tr>
            <tr>
                <th>Iva</th>
                <td>{{ producto.iva }}</td>
            </tr>
            <tr>
                <th>IDD</th>
                <td>{{ producto.IDD }}</td>
            </tr>
            <tr>
                <th>Stock</th>
                <td>{{ producto.Stock }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_producto_index') }}">back to list</a>

    <a href="{{ path('app_producto_edit', {'id': producto.id}) }}">edit</a>

    {{ include('producto/_delete_form.html.twig') }}
{% endblock %}
